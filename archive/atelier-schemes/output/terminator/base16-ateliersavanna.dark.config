    copy_on_selection = True
    allow_bold = False
    palette = "#171c19:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#87928a:#5f6d64:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#ecf4ee"
