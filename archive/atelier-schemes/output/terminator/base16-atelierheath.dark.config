[global_config]
  title_transmit_bg_color = "#ca402b"
  title_inactive_bg_color = "#776977"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1b181b:#ca402b:#918b3b:#bb8a35:#516aec:#7b59c0:#159393:#ab9bab:#776977:#ca402b:#918b3b:#bb8a35:#516aec:#7b59c0:#159393:#f7f3f7"
    foreground_color = "#ab9bab"
    background_color = "#1b181b"
    cursor_color = "#ab9bab"

  [[Base16 Atelier Heath Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1b181b:#ca402b:#918b3b:#bb8a35:#516aec:#7b59c0:#159393:#ab9bab:#776977:#ca402b:#918b3b:#bb8a35:#516aec:#7b59c0:#159393:#f7f3f7"
    foreground_color = "#ab9bab"
    background_color = "#1b181b"
    cursor_color = "#ab9bab"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Heath Dark
    [[[window0]]]
      type = Window
      parent = ""
