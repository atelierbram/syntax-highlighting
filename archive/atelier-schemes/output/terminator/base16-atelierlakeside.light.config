[global_config]
  title_transmit_bg_color = "#d22d72"
  title_inactive_bg_color = "#5a7b8c"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#161b1d:#d22d72:#568c3b:#8a8a0f:#257fad:#6b6bb8:#2d8f6f:#7ea2b4:#5a7b8c:#d22d72:#568c3b:#8a8a0f:#257fad:#6b6bb8:#2d8f6f:#ebf8ff"
    foreground_color = "#516d7b"
    background_color = "#ebf8ff"
    cursor_color = "#516d7b"

  [[Base16 Atelier Lakeside Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#ebf8ff:#d22d72:#568c3b:#8a8a0f:#257fad:#6b6bb8:#2d8f6f:#7ea2b4:#5a7b8c:#d22d72:#568c3b:#8a8a0f:#257fad:#6b6bb8:#2d8f6f:#161b1d"
    foreground_color = "#516d7b"
    background_color = "#ebf8ff"
    cursor_color = "#516d7b"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Lakeside Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
