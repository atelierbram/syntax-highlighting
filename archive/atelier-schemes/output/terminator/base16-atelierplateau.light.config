[global_config]
  title_transmit_bg_color = "#ca4949"
  title_inactive_bg_color = "#655d5d"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1b1818:#ca4949:#4b8b8b:#a06e3b:#7272ca:#8464c4:#5485b6:#8a8585:#655d5d:#ca4949:#4b8b8b:#a06e3b:#7272ca:#8464c4:#5485b6:#f4ecec"
    foreground_color = "#585050"
    background_color = "#f4ecec"
    cursor_color = "#585050"

  [[Base16 Atelier Plateau Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#f4ecec:#ca4949:#4b8b8b:#a06e3b:#7272ca:#8464c4:#5485b6:#8a8585:#655d5d:#ca4949:#4b8b8b:#a06e3b:#7272ca:#8464c4:#5485b6:#1b1818"
    foreground_color = "#585050"
    background_color = "#f4ecec"
    cursor_color = "#585050"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Plateau Light
    [[[window0]]]
      type = Window
      parent = ""
