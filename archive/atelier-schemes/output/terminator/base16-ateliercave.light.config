[global_config]
  title_transmit_bg_color = "#be4678"
  title_inactive_bg_color = "#655f6d"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#19171c:#be4678:#2a9292:#a06e3b:#576ddb:#955ae7:#398bc6:#8b8792:#655f6d:#be4678:#2a9292:#a06e3b:#576ddb:#955ae7:#398bc6:#efecf4"
    foreground_color = "#585260"
    background_color = "#efecf4"
    cursor_color = "#585260"

  [[Base16 Atelier Cave Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#efecf4:#be4678:#2a9292:#a06e3b:#576ddb:#955ae7:#398bc6:#8b8792:#655f6d:#be4678:#2a9292:#a06e3b:#576ddb:#955ae7:#398bc6:#19171c"
    foreground_color = "#585260"
    background_color = "#efecf4"
    cursor_color = "#585260"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Cave Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
