[global_config]
  title_transmit_bg_color = "#e6193c"
  title_inactive_bg_color = "#687d68"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#131513:#e6193c:#29a329:#98981b:#3d62f5:#ad2bee:#1999b3:#8ca68c:#687d68:#e6193c:#29a329:#98981b:#3d62f5:#ad2bee:#1999b3:#f4fbf4"
    foreground_color = "#5e6e5e"
    background_color = "#f4fbf4"
    cursor_color = "#5e6e5e"

  [[Base16 Atelier Seaside Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#f4fbf4:#e6193c:#29a329:#98981b:#3d62f5:#ad2bee:#1999b3:#8ca68c:#687d68:#e6193c:#29a329:#98981b:#3d62f5:#ad2bee:#1999b3:#131513"
    foreground_color = "#5e6e5e"
    background_color = "#f4fbf4"
    cursor_color = "#5e6e5e"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Seaside Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
